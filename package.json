{
  "name": "vue3-tree",
  "version": "0.1.0",
  "main": "./dist/vue3-tree.umd.js",
  "module": "./dist/vue3-tree.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-tree.es.js",
      "require": "./dist/vue3-tree.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "typecheck": "vue-tsc --project tsconfig.types.json --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar/vue-language-plugin-pug": "^1.0.24",
    "@vue/tsconfig": "^0.1.3",
    "pug": "^3.0.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  }
}
